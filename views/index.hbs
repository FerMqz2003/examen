{{> header}}

<div class="container">
<h1>Pintores</h1>
{{#if pintores}}
<table class=" table table-dark table-hover">
    <thead>
        {{#each pintores}}
        <tr>
            <td>{{_id}}</td>
            <td>{{nombre}}</td>
            <td>{{nacionalidad}}</td>
            <td>{{corriente}}</td>
            <td>{{pintura}}</td>
        </tr>
        {{/each}}
    </thead>
    <tbody>
        <tr></tr>
    </tbody>
</table>
  {{else}}
    <div>No se encuentran pintores</div>
    {{/if}}
</div>
{{> footer }}